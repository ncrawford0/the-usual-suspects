<div class="content-splash">
  <div class="container">
    <div class="column">
        <h1>All Bars</h1>

        <ul>
          <% @bars.each do |bar| %>
          <li><%= link_to bar.name, bar_path(bar) %></li>
          <% end %>
        </ul>

        <%= button_to "Add New Bar", new_bar_path, method: :get %>
    </div>
  </div>
</div>
<div class="pagination">
<%= paginate @bars %>
</div>
